{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "scan:deps": "depcheck --ignore-dirs=dist,build",
    "scan:unused": "unimported --flow",
    "scan:orphans": "madge src --extensions js,jsx --orphans",
    "cleanup:safe": "bash scripts/safe-cleanup.sh",
    "scan:circular": "madge src --extensions js,jsx --circular",
    "scan:graph": "madge src --extensions js,jsx --image graph.png",
    "scan:all": "npm run scan:deps && npm run scan:unused && npm run scan:orphans && npm run scan:circular",
    "health": "npm run lint && npm run scan:all"
  },
  "dependencies": {
    "fuse.js": "^7.1.0",
    "mammoth": "^1.11.0",
    "pdfjs-dist": "^5.4.149",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "knip": "^5.66.0",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "unimported": "^1.31.0",
    "vite": "^7.1.7"
  }
}
